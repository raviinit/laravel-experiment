{
    "name": "reportico/laravel-experiment",
    "description": "",
    "authors": [
        {
            "name": "Reportico",
            "email": "info@reportico.org"
        }
    ],
    "license": "GPL-3.0",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.2",
        "reportico/reportico": "dev-new-architecture-twig"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers"
        ],
        "files": [
            "src/Reportico/Laravel/ReporticoSession.php"
        ],
        "psr-0": {
            "Reportico\\Laravel\\": "src/"
        },
        "psr-4": {
            "Reportico\\Engine\\": "reportico/reportico/src"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    }

}
